{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "redirects": [
    {
      "source": "/post/:path(.*)",
      "destination": "/posts/:path",
      "permanent": true
    },
    {
      "source": "/(archive\\/?.*)",
      "destination": "/posts",
      "permanent": true
    }
  ]
}
